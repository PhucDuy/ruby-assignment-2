<h1>Sent Messages</h1>
<table class="table table-stripped">
  <thead class="thead thead-default">
    <tr>
      <th>Sender</th>
      <th>Body</th>
      <th>Read at</th>
    </tr>
  </thead>
  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.sender.name%></td>
        <td><%= message.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class="row">
  <div class="col-md-8">
    <%= link_to "Back", messages_path , class: "btn btn-primary"%>
  </div>
</div>